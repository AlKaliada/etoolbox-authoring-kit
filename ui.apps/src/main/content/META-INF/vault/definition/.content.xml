<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:vlt="http://www.day.com/jcr/vault/1.0"
          xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="vlt:PackageDefinition"
          jcr:description="AEM Authoring Toolkit - UI Applications">
    <thumbnail.png/>
    <filter jcr:primaryType="nt:unstructured">
        <f0 jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/apps/authoring-toolkit/clientlib-injector"
            rules="[]"/>
        <f1 jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/apps/authoring-toolkit/lists"
            rules="[]"/>
        <f2 jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/apps/authoring-toolkit/depends-on"
            rules="[]"/>
        <f3 jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/apps/authoring-toolkit/depends-on-injector"
            rules="[]"/>
        <f4 jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/apps/authoring-toolkit/install"
            rules="[]"/>
        <f5 jcr:primaryType="nt:unstructured"
            mode="replace"
            root="/apps/authoring-toolkit/fixes"
            rules="[]"/>
    </filter>
    <screenshots jcr:primaryType="nt:unstructured"/>
</jcr:root>
