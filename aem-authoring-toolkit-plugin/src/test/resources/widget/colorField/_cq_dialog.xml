<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="cq/gui/components/authoring/dialog"
          jcr:title="test-component-dialog"
          height="{Double}480.0"
          width="{Double}560.0">
    <content jcr:primaryType="nt:unstructured" sling:resourceType="granite/ui/components/coral/foundation/container">
        <layout jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <color jcr:primaryType="nt:unstructured"
                           sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
                           name="./color"
                           value="HEX"
                           autogenerateColors="shades"
                           disabled="{Boolean}false"
                           emptyText="test-string"
                           renderHidden="{Boolean}false"
                           required="{Boolean}false"
                           showSwatches="{Boolean}false"
                           showProperties="{Boolean}true"
                           showDefaultColors="{Boolean}false"
                           validation="[foundation.jcr.name]"
                           variant="swatch"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>