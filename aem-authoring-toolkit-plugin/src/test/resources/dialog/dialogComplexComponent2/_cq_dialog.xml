<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="First dialog title"
    sling:resourceType="cq/gui/components/authoring/dialog"
    height="{Double}480.0"
    width="{Double}560.0"
    extraClientlibs="[cq.common.wcm]">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <firstTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="First tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <layout
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="{Boolean}false"
                                fieldDescription="description_layout"
                                fieldLabel="label_layout"
                                name="./field_layout"
                                renderHidden="{Boolean}false"
                                required="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <left
                                        jcr:primaryType="nt:unstructured"
                                        selected="{Boolean}true"
                                        text="First option"
                                        value="left"/>
                                    <right
                                        jcr:primaryType="nt:unstructured"
                                        text="Second option"
                                        value="right"/>
                                    <stacked
                                        jcr:primaryType="nt:unstructured"
                                        text="Third option"
                                        value="stacked"/>
                                    <field_layout
                                        jcr:primaryType="nt:unstructured"
                                        text="Forth option"
                                        value="field_layout"/>
                                </items>
                            </layout>
                            <gradient
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                autosubmit="{Boolean}false"
                                checked="{Boolean}true"
                                disabled="{Boolean}false"
                                fieldDescription="description enable gradient"
                                fieldLabel="label enable gradient"
                                name="./fieldEnableGradient"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                text="label enable gradient"
                                uncheckedValue="{Boolean}false"
                                value="{Boolean}true"/>
                        </items>
                    </firstTab>
                    <secondTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Second tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </secondTab>
                    <thirdTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Third tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </thirdTab>
                    <fourthTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Fourth tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </fourthTab>
                    <fifthTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Fifth tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </fifthTab>
                    <sixthTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Sixth tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </sixthTab>
                    <seventhTab
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Seventh tab"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </seventhTab>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
