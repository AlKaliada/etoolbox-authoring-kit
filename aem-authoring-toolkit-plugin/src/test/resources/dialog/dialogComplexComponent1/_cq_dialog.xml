<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Component"
    sling:resourceType="cq/gui/components/authoring/dialog"
    extraClientlibs="[cq.common.wcm,core.wcm.page.properties,cq.wcm.msm.properties]"
    height="{Double}480.0"
    width="{Double}800.0">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs">
                <items jcr:primaryType="nt:unstructured">
                    <tab_1
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_1"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <firstField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}false"
                                fieldDescription="First field description"
                                fieldLabel="First field label"
                                name="./first_field"
                                renderHidden="{Boolean}false"
                                validation="[foundation.jcr.name]"
                                required="{Boolean}true"/>
                            <secondField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="cq/gui/components/authoring/dialog/richtext"
                                disabled="{Boolean}false"
                                fieldDescription="Second field description"
                                fieldLabel="Second field label"
                                name="./second_field"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                validation="[foundation.jcr.name1,foundation.jcr.name2]"
                                useFixedInlineToolbar="{Boolean}true">
                                <uiSettings jcr:primaryType="nt:unstructured">
                                    <cui jcr:primaryType="nt:unstructured">
                                        <inline
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                        <fullscreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                        <dialogFullScreen
                                            jcr:primaryType="nt:unstructured"
                                            toolbar="[links#modifylink,links#unlink,subsuperscript#subscript,subsuperscript#superscript]"/>
                                    </cui>
                                </uiSettings>
                                <rtePlugins jcr:primaryType="nt:unstructured">
                                    <subsuperscript
                                        jcr:primaryType="nt:unstructured"
                                        features="[subscript,superscript]"/>
                                    <links
                                        jcr:primaryType="nt:unstructured"
                                        features="[unlink,modifylink]"/>
                                    <styles jcr:primaryType="nt:unstructured"/>
                                </rtePlugins>
                            </secondField>
                            <thirdField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                disabled="{Boolean}false"
                                fieldDescription="Third field description"
                                fieldLabel="Third field label"
                                name="./third_Field"
                                renderHidden="{Boolean}false"
                                required="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <item1
                                        jcr:primaryType="nt:unstructured"
                                        text="First"
                                        value="1"/>
                                    <item2
                                        jcr:primaryType="nt:unstructured"
                                        selected="{Boolean}true"
                                        text="Second"
                                        value="2"/>
                                    <item3
                                        jcr:primaryType="nt:unstructured"
                                        text="Third"
                                        value="3"/>
                                    <item4
                                        jcr:primaryType="nt:unstructured"
                                        text="Fourth"
                                        value="4"/>
                                </items>
                            </thirdField>
                        </items>
                    </tab_1>
                    <tab_2
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_2"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </tab_2>
                    <tab_3
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_3"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </tab_3>
                    <tab_4
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_4"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </tab_4>
                    <tab_5
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_5"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured"/>
                    </tab_5>
                    <tab_6
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_6"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <exampleFirstNumberField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}false"
                                fieldDescription="First number field description"
                                fieldLabel="First number field label"
                                max="{Double}1.7976931348623157E308"
                                min="{Double}0.0"
                                name="./first_number_field"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                step="{Double}1.0"
                                value="0"/>
                            <exampleSecondNumberField
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                disabled="{Boolean}false"
                                fieldDescription="Second number field description"
                                fieldLabel="Second number field label"
                                max="{Double}1.7976931348623157E308"
                                min="{Double}0.0"
                                name="./second_number_field"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false"
                                step="{Double}1.0"
                                value="0"/>
                        </items>
                    </tab_6>
                    <tab_7
                        granite:hidden="{Boolean}false"
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Tab_7"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <exampleFieldSet
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
                                disabled="{Boolean}false"
                                name="./exampleFieldSet"
                                renderHidden="{Boolean}false"
                                required="{Boolean}false">
                                <items jcr:primaryType="nt:unstructured">
                                    <content
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        disabled="{Boolean}false"
                                        fieldDescription="Content description"
                                        fieldLabel="Content label"
                                        name="./Content"
                                        renderHidden="{Boolean}false"
                                        required="{Boolean}false"/>
                                </items>
                            </exampleFieldSet>
                        </items>
                    </tab_7>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
